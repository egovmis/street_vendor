package com.medical.app.serviceImp;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

import com.medical.app.bean.StaffBean;
import com.medical.app.repository.StaffRepository;
import com.medical.app.service.StaffService;

@Service
@Transactional
public class StaffServiceImp implements StaffService{
	@Autowired
	StaffRepository staffRepository;
	
	@Override
	public List<StaffBean> findAllStaff() {
		// TODO Auto-generated method stub
		return (List<StaffBean>) staffRepository.findAll();
	}

}
